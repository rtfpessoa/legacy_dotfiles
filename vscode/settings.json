{
  "editor.fontFamily": "'Meslo LG S DZ for Powerline', 'Source Code Pro', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "explorer.confirmDelete": false,
  "files.associations": {
    "*.env.development": "env",
    "*.env.production": "env"
  },
  "files.exclude": {
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/_build": true,
    "**/.elixir_ls": true
  },
  "files.insertFinalNewline": true,
  "files.watcherExclude": {
    "**/.ammonite": true,
    "**/.bloop": true,
    "**/.metals": true
  },
  "editor.renderWhitespace": "trailing",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[hocon]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[terraform]": {
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "go.useLanguageServer": false,
  "go.formatTool": "goimports",
  "go.autocompleteUnimportedPackages": true,
  "editor.codeActionsOnSave": {},
  "typescript.updateImportsOnFileMove.enabled": "always",
  "cSpell.userWords": [
    "deserialization",
    "oapi",
    "openapi",
    "openid",
    "roadmaps",
    "rtfpessoa",
    "structs",
    "unmarshall"
  ],
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "window.zoomLevel": 0,
  "terminal.integrated.fontFamily": "'Meslo LG S DZ for Powerline', 'Source Code Pro', 'Droid Sans Mono', monospace, 'Droid Sans Fallback'",
  "terminal.integrated.shell.linux": "/usr/bin/fish",
  "window.autoDetectColorScheme": true,
  "workbench.colorCustomizations": {},
  "workbench.startupEditor": "newUntitledFile",
  "workbench.preferredLightColorTheme": "GitHub Light",
  "workbench.preferredDarkColorTheme": "GitHub Dark",
  "workbench.sideBar.location": "left",
  "workbench.colorTheme": "Default Dark+"
}
